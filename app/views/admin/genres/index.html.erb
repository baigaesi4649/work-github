<p>ジャンル一覧・追加</p>
  <h4>ジャンル名</h4>
<%= form_with model: Genre.new , url: admin_genres_path do |f| %>
  <%= f.text_field :name %>
  <%= f.submit '新規登録' %>
<% end %>
<table class="table">
<tbody>
<tr>
  <th>ジャンル</th>
  <th></th>
</tr>
<% @genres.each do |genre| %>
<tr>
  <td><%= genre.name %></td>
  <td><%= link_to "編集する",edit_admin_genre_path(genre.id),class: "btn btn btn-success btn-sm" %></td>
</tr>
<% end %>
</tbody>
</table>