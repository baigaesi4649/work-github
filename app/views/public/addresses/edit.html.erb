<div class="p-3 mb-2 bg-light text-dark">配送先編集</div>
<%= form_with model: @address, url: public_address_path, method: :patch do |f| %>
<table class="table">
 <tbody>
  <tr>
   <td>郵便番号</td>
   <td><%= f.text_field :postal_code %></td>
  </tr>
  <tr>
   <td>住所</td>
   <td><%= f.text_field :address %></td>
  </tr>
  <tr>
   <td>宛名</td>
   <td><%= f.text_field :name %></td>
  </tr>
 </tbody>
</table>
  <%= f.submit '変更内容を保存',class: "btn btn btn-success btn-sm" %>
<% end %>