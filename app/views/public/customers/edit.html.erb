会員情報編集
<%= form_with model: @customer, url: public_customers_information_path, method: :patch do |f| %>
<table class="table">
 <tbody>
  <td>氏名</td>
  <td><%= f.text_field :last_name %></td>
  <td><%= f.text_field :first_name %></td>
  </tr>
  <tr>
   <td>フリガナ</td>
   <td><%= f.text_field :last_name_kana %></td>
   <td><%= f.text_field :first_name_kana %></td>
  </tr>
  <tr>
   <td>郵便番号</td>
   <td><%= f.text_field :postal_code %></td>
  </tr>
  <tr>
   <td>住所</td>
   <td><%= f.text_field :address %></td>
  </tr>
  <tr>
   <td>電話番号</td>
   <td><%= f.telephone_field :telephone_number %></td>
  </tr>
  <tr>
   <td>メールアドレス</td>
   <td><%= f.email_field :email %></td>
  </tr>
 </tbody>
</table>
  <%= f.submit '編集内容を保存' %>
<% end %>
<%= link_to "退会する", public_customers_unsubscribe_path,class: "btn btn btn-primary btn-sm" %>